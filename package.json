{
  "name": "mongo-select",
  "version": "0.1.3",
  "description": "Module to help create projection objects when working with mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "prepublish": "tsc --build tsconfig.json",
    "test": "mocha --reporter list",
    "pretest": "find dist test -name '*.js' -print0 | xargs -0 jshint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dschenkelman/mongo-select"
  },
  "keywords": [
    "mongodb",
    "projetions"
  ],
  "author": "Damian Schenkelman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dschenkelman/mongo-select/issues"
  },
  "jshintConfig": {
    "strict": "true",
    "globalstrict": "true",
    "esversion": 6
  },
  "devDependencies": {
    "jshint": "^2.12.0",
    "mocha": "^8.4.0",
    "should": "^13.2.3",
    "typescript": "^4.3.2"
  }
}
